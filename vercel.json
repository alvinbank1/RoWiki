{
  "version": 2,
  "builds": [
      {
          "src": "app.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSsize":"15mb", "runtime": "python3.9"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
        }
      }
  ],
  "routes": [
      {
          "src": "(.*)",
          "dest": "app.py"
      }
  ]
}